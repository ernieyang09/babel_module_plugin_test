{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "import",
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "globals": {
    "v": true,
    "env": true
  },
  "rules": {
    "no-console": 0,
    "react/prefer-stateless-function": [ 0, {
      "ignorePureComponents": true
    }]
  }
}
